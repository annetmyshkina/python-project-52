msgid "First name"
msgstr "Имя"

#: task_manager/templates/users/users_list.html:22 users/forms.py:23
msgid "Last name"
msgstr "Фамилия"

#: task_manager/templates/users/users_list.html:23 users/forms.py:32
msgid "Username"
msgstr "Имя пользователя"

#: task_manager/templates/users/users_list.html:24
msgid "Joined"
msgstr "Присоединился"

#: task_manager/templates/users/users_list.html:25
msgid "Actions"
msgstr "Действия"

#: task_manager/templates/users/users_list.html:36
msgid "You"
msgstr "Вы"

#: task_manager/templates/users/users_list.html:46
msgid "Edit"
msgstr "Редактировать"

#: task_manager/templates/users/users_list.html:62
msgid "No users found"
msgstr "Пользователи не найдены"

#: users/forms.py:16
msgid "Enter your first name"
msgstr "Введите свое имя"

#: users/forms.py:25
msgid "Enter your last name"
msgstr "Введите свою фамилию"

#: users/forms.py:33
msgid "Invalid characters"
msgstr "Недопустимые символы"

#: users/forms.py:35
msgid "Enter username"
msgstr "Введите имя пользователя"

#: users/forms.py:37
msgid "Required. 150 characters or fewer. Letters, digits and @/./+/-/_ only."
msgstr "Длина не более 150 символов. Только буквы, цифры и @/./+/-/_."

#: users/forms.py:52
msgid "Username already taken."
msgstr "Имя пользователя уже занято."

#: users/forms.py:66
msgid "Password"
msgstr "Пароль"

#: users/forms.py:67
msgid "Your password must contain at least 8 characters."
msgstr "Ваш пароль должен содержать не менее 8 символов."

#: users/forms.py:69
msgid "Confirm password"
msgstr "Подтвердите пароль"

#: users/forms.py:70
msgid "Please enter the password again to confirm."
msgstr "Пожалуйста, введите пароль еще раз для подтверждения."

#: users/forms.py:84
msgid "I confirm this action is irreversible"
msgstr "Я подтверждаю, что это действие необратимо"

